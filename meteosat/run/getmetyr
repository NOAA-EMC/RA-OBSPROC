set -eua

year=$1; dest=$2; mkdir -p $dest; cd $dest

grep $year ../fix/metsats|\
while read line; do
mfile=$(echo $line|cut -d' ' -f1)
htar -Hnostage -xvf $ec5/$mfile-CMSAF-NETC \*$year\*
done

year=$((year-1)); mnth=12

grep $year ../fix/metsats|\
while read line; do
mfile=$(echo $line|cut -d' ' -f1)
htar -Hnostage -xvf $ec5/$mfile-CMSAF-NETC \*$year$mnth\*
done

